{
  "fontFamily": {
    "primary": "Poppins",
    "secondary": "Inter"
  },
  "fontWeight": {
    "regular": 400,
    "medium": 500,
    "semibold": 600,
    "bold": 700
  },
  "fontSize": {
    "xs": 10,
    "sm": 12,
    "md": 16,
    "mmd":18,
    "lg": 20,
    "xl": 24,
    "xxl": 32
  },
  "styles": {
    "h1": {
      "fontFamily": "primary",
      "fontSize": "xl",
      "fontWeight": "bold",
      "lineHeight": 28.8,
      "letterSpacing": 0
    },
    "h2": {
      "fontFamily": "primary",
      "fontSize": "lg",
      "fontWeight": "bold",
      "lineHeight": 24,
      "letterSpacing": 0
    },
    "body": {
      "fontFamily": "primary",
      "fontSize": "mmd",
      "fontWeight": "semibold",
      "lineHeight": 21.6,
      "letterSpacing": 0
    },
    "button": {
      "default": {
        "fontFamily": "primary",
        "fontSize": "md",
        "fontWeight": "medium",
        "lineHeight": 19.2,
        "letterSpacing": 0
      },
      "small": {
        "fontFamily": "primary",
        "fontSize": "sm",
        "fontWeight": "medium",
        "lineHeight": 14.4,
        "letterSpacing": 0
      }
    }
  }
}